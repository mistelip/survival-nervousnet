<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cpcrasher.survival">

    <uses-permission android:name="ch.ethz.coss.nervousnet.hub.BIND_PERM" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AccelerometerActivity"
            android:label="@string/title_activity_accelerometer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".TsunamiActivity" />
        <activity android:name=".TerroristActivity" />
        <activity android:name=".EarthquakeActivity"></activity>
    </application>

</manifest>